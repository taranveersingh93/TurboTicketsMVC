                            <div class="project-info">
                                <form asp-area="Identity" asp-page="/Account/Login" method="post">
                                    <small>Supervises Projects</small>
                                    <h3>
                                        <button class="border-0" type="submit" name="demoEmail" value="DemoPMEmail">
                                            <span class="text-primary">Project Manager</span>
                                            <i class="fa fa-angle-right project-icon"></i>
                                        </button>
                                    </h3>

                                </form>

                            </div>
                        </div>


                        <div class="item project-wrapper" data-aos="fade-up">
                            <img src="~/img/developer.jpg" class="img-fluid" alt="project image">

                            <div class="project-info">
                                <form asp-area="Identity" asp-page="/Account/Login" method="post">
                                    <small>Responsible for Ticket Progress</small>
                                    <h3>
                                        <button class="border-0" type="submit" name="demoEmail" value="DemoDevEmail">
                                            <span class="text-primary">Developer</span>
                                            <i class="fa fa-angle-right project-icon"></i>
                                        </button>
                                    </h3>

                                </form>

                            </div>
                        </div>

                        <div class="item project-wrapper" data-aos="fade-up">
                            <img src="~/img/submitter.jpg" class="img-fluid" alt="project image">

                            <div class="project-info">
                                <form asp-area="Identity" asp-page="/Account/Login" method="post">
                                    <small>Ticket Creator</small>
                                    <h3>
                                        <button class="border-0" type="submit" name="demoEmail" value="DemoSubEmail">
                                            <span class="text-primary">Submitter</span>
                                            <i class="fa fa-angle-right project-icon"></i>
                                        </button>
                                    </h3>

                                </form>

                            </div>
                        </div>


                    </div>
                </div>

            </div>
        </div>
    </section>

    <section class="features section-padding" id="features">
        <div class="container">
            <div class="row">
                <div class="col-lg-7 mx-auto col-md-10 col-12">
                    <h2 class="text-light" data-aos="fade-up">All Features</h2>
                    <div class="accordion" id="accordion">
                        <div class="accordion-item">
                            <h2 class="accordion-header" id="headingOne">
                                <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
